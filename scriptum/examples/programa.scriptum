mutabilis numerus_principal: numerus = 3;
constans mensagem: textus = "Iteratio";

dum (numerus_principal > 0) {
    imprime(mensagem + ": " + numerus_principal);
    numerus_principal = numerus_principal - 1;
}

si (numerus_principal == 0) {
    imprime("Concluido!");
} aliter {
    imprime("Erro no contador");
}
